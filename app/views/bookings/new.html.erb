<%= simple_form_for [@car, @booking] do |f| %>
  <%= f.input :start_date, as: :string, html5: true %>
  <%= f.input :end_date, as: :string, html5: true %>
  <%= f.input :comment, html5: true %>
  <%= f.submit 'Book', class: "btn btn-default btn-primary"  %>
<% end %>

<br>

<%= link_to "Back", car_path(@car) %>